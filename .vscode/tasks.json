{
	"version": "2.0.0",
	"tasks": [
		{
			
			"label": "build",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"build",
				"/property:GenerrateFullPaths=true",
				"/consoleloggerparameters:NoSummary",
			],
			"group": "build",
			"presentation": {
				"reveal": "silent",
			},
			"problemMatcher": "$msCompile",
		},
		{
			"label": "launch godot",
			"type": "shell",
			"command": "C:/Program Files/Godot/Godot_v4.2.1-stable_mono_win64/Godot_v4.2.1-stable_mono_win64.exe",
			"args": [
				"--path",
				"${workspaceFolder}",
				"--editor"
			],
			"problemMatcher": []
		}
	]
}